<h2>History</h2>

<table border=1>
  <thead>
    <tr>
      <th>Type</th>
      <th>Version</th>
      <th>Data</th>
      <th>Meta Data</th>
      <th>Created At</th>
    </tr>
  </thead>
  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.type %></td>
        <td><%= event.version %></td>
        <td><%= debug event.data %></td>
        <td><%= debug event.metadata %></td>
        <td><%= event.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
